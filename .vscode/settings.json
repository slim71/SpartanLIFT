{
  "C_Cpp.clang_format_path": "/usr/bin/clang-format-17",
  "C_Cpp.clang_format_style": "file:${workspaceFolder}/.vscode/.clang-format",
  "C_Cpp.default.cStandard": "c17",
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
  "C_Cpp.formatting": "clangFormat",
  "C_Cpp.intelliSenseEngine": "default",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "autopep8.args": [
    "\"--max-line-length=100\""
  ],
  "clang-format.executable": "/usr/bin/clang-format-17",
  "editor.foldingStrategy": "indentation",
  "python.analysis.extraPaths": [
    "/opt/ros/humble/lib/python3.10/site-packages",
    "/opt/ros/humble/local/lib/python3.10/dist-packages"
  ],
  "python.analysis.typeCheckingMode": "basic",
  "python.autoComplete.extraPaths": [
    "/opt/ros/humble/lib/python3.10/site-packages",
    "/opt/ros/humble/local/lib/python3.10/dist-packages"
  ],
  "python.envFile": "${workspaceFolder}/.env",
  "ros.distro": "humble",
  "ros.isolateEnvironment": false,
  "ros.rosSetupScript": "${workspaceFolder}/install/setup.bash",
  "search.exclude": {
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/build": true,
    "**/install": true,
    "**/log": true,
    "**/node_modules": true
  },
  "uncrustify.configPath.linux": "/opt/ros/humble/lib/python3.10/site-packages/ament_uncrustify/configuration/ament_code_style.cfg",
  "editor.foldingImportsByDefault": true,
  "clang-format.fallbackStyle": "",
  "C_Cpp.default.compilerPath": "/usr/bin/gcc",
  "files.associations": {
    "*.tpp": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "csignal": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "any": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "strstream": "cpp",
    "bit": "cpp",
    "*.tcc": "cpp",
    "bitset": "cpp",
    "charconv": "cpp",
    "chrono": "cpp",
    "cinttypes": "cpp",
    "codecvt": "cpp",
    "compare": "cpp",
    "complex": "cpp",
    "concepts": "cpp",
    "condition_variable": "cpp",
    "cstdint": "cpp",
    "deque": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "map": "cpp",
    "set": "cpp",
    "string": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "vector": "cpp",
    "exception": "cpp",
    "algorithm": "cpp",
    "functional": "cpp",
    "iterator": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "ratio": "cpp",
    "regex": "cpp",
    "string_view": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "hash_map": "cpp",
    "hash_set": "cpp",
    "fstream": "cpp",
    "future": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "numbers": "cpp",
    "ostream": "cpp",
    "ranges": "cpp",
    "semaphore": "cpp",
    "shared_mutex": "cpp",
    "span": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "stop_token": "cpp",
    "streambuf": "cpp",
    "thread": "cpp",
    "cfenv": "cpp",
    "typeindex": "cpp",
    "typeinfo": "cpp",
    "valarray": "cpp",
    "variant": "cpp",
    "core": "cpp"
  },
  "todo-tree.general.debug": true,
  "todo-tree.tree.scanMode": "workspace only",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]",
    "CHECK"
  ],
  "todo-tree.filtering.excludeGlobs": [
    "**/PX4-Autopilot/**",
    "**/src/Micro-XRCE-DDS-Agent/**",
    "**/src/px4_msgs/**",
    "**/src/px4_ros_com/**",
    "**/build/**",
    "**/.vscode/**",
    "**/.github/**",
    "**/_output/**"
  ]
}
