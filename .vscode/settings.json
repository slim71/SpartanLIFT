{
  "C_Cpp.clang_format_path": "/usr/bin/clang-format-17",
  "C_Cpp.clang_format_style": "file:${workspaceFolder}/.vscode/.clang-format",
  "C_Cpp.default.cStandard": "c17",
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
  "C_Cpp.formatting": "clangFormat",
  "C_Cpp.intelliSenseEngine": "default",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "autopep8.args": [
    "\"--max-line-length=100\""
  ],
  "clang-format.executable": "/usr/bin/clang-format-17",
  "editor.foldingStrategy": "indentation",
  "python.analysis.extraPaths": [
    "/opt/ros/humble/lib/python3.10/site-packages",
    "/opt/ros/humble/local/lib/python3.10/dist-packages"
  ],
  "python.analysis.typeCheckingMode": "basic",
  "python.autoComplete.extraPaths": [
    "/opt/ros/humble/lib/python3.10/site-packages",
    "/opt/ros/humble/local/lib/python3.10/dist-packages"
  ],
  "python.envFile": "${workspaceFolder}/.env",
  "ros.distro": "humble",
  "ros.isolateEnvironment": false,
  "ros.rosSetupScript": "${workspaceFolder}/install/setup.bash",
  "search.exclude": {
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/build": true,
    "**/install": true,
    "**/log": true,
    "**/node_modules": true
  },
  "uncrustify.configPath.linux": "/opt/ros/humble/lib/python3.10/site-packages/ament_uncrustify/configuration/ament_code_style.cfg",
  "editor.foldingImportsByDefault": true,
  "clang-format.fallbackStyle": "",
  "C_Cpp.default.compilerPath": "/usr/bin/gcc",
  "files.associations": {
    "*.tpp": "cpp"
  }
}
